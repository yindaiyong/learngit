  <update id="updateAccountData"  parameterType="java.util.List">
	<foreach collection="list" item="item" index="index" open="begin" close=";end;" separator=";">
	       update d_account_req d
		   set d.ar_return_code     = '0000',
		       d.ar_valid_flag      = '01',
		       d.ar_ta_trans_status = '00',
		       d.ar_error_dec = '',
		       d.ar_uuser = #{item.USER,jdbcType = VARCHAR},
		       d.ar_utime = #{item.SYSDATE,jdbcType = DATE}
		   where d.ar_id =#{item.ID,jdbcType = VARCHAR}
	</foreach>	   
		   
  </update>
  
  <select id="selectAccountReqOpenCount" parameterType="Map" resultType="Integer">
		select count(1) from d_account_req t 
		where t.ar_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
		and t.ar_trans_date = #{BUSINESSDATE,jdbcType = VARCHAR}
