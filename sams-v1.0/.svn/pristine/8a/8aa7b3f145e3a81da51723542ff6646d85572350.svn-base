	 	<if test="condition.channelCode != null and condition.channelCode != '' ">
			and ar_channel_code=#{condition.channelCode,jdbcType=VARCHAR}
	 	</if>
	 	<if test="condition.certificateno != null and condition.certificateno != '' ">
