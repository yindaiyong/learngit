  </select>

  <select id="selectAccountReq" parameterType="map" resultMap="tempResultMapReq">
  		select * from d_account_req t where t.ar_channel_code = #{CHANNELCODE,jdbcType = VARCHAR} AND T.AR_TRANSACTION_ACCOUNT_ID = #{TRANSACTIONACCOUNTID,jdbcType = VARCHAR}
