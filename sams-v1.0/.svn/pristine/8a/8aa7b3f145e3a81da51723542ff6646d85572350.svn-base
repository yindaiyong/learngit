	 <!--  ,  -->
	 nvl(nvl(nvl(t.ar_mobile_tel_no,t.ar_home_tel_no),t.ar_tel_no),t.ar_office_tel_no) telno,
	 t.ar_channel_code channelcode,
	 t.ar_error_dec errordec,
	 t.ar_return_code returnCode
  	from d_account_req  t left join p_channel_info p on t.ar_channel_code = p.ci_channel_code 
  	<where>
	 	<if test="condition.transDate != null and condition.transDate != ''">
