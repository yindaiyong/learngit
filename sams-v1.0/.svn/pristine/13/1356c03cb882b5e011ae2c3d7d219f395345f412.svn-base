		<select id="findMarketDetailCondition" resultMap="BaseResultMap" parameterType="hashMap" >
    select
        <include refid="BaseColumnListToOrder"/>
        from D_FUND_MARKET_CFM where 1=1
        <if test="condition.channelCode != null and condition.channelCode != ''  ">
                and FM_CHANNEL_CODE = #{condition.channelCode,jdbcType=VARCHAR}
        </if>
        
        <!-- <if test=" condition.transDate!= null and condition.transDate != ''  "> -->
                and FM_BUSINESS_DATE = #{condition.transDate,jdbcType=VARCHAR} 
        <!-- </if> -->
         
        <if test="pageInfo.sort != null and pageInfo.sort != ''">
            order by ${pageInfo.sort} ${pageInfo.order}
        </if>
	</select>
  
    
	<select id="selectHoldShares" resultType="string" parameterType="hashMap" >
     select sum(ts.f_remainshares)
        from ta_ttrustcontractdetails tt, ta_tsharedetail ts
       where tt.l_serialno = ts.l_contractserialno
        and tt.c_trustagencyno = #{CHANNELCODE,jdbcType=VARCHAR}
        and ts.c_fundcode = #{FUNDCODE,jdbcType=VARCHAR}
	</select>
  
  <select id="getFundMarketDataInfo" parameterType="hashMap" resultType="hashMap">
   SELECT <include refid="Base_Column_List_File" />
       from  d_fund_market_cfm t
	   <where>
    <if test="CHANNELCODE != null" >
       and t.fm_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
    </if>
    <if test="TRANSDATE != null" >
       AND t.fm_business_date = #{TRANSDATE,jdbcType = VARCHAR}
    </if>
    </where>
     order by t.fm_fund_code 
  </select>
  
