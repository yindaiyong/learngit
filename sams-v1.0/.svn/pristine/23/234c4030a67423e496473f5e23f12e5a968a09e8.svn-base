  <select id="selectAccountReq" resultMap="BaseResultAccountReq">
    select <include refid="Base_Column_AccountReq"></include>
    from d_account_req where ar_channel_code = #{channelCode}
    and ar_certificate_no = #{certId}
    <if test="certType != null">
      and ar_certificate_type = #{certType}
    </if>
    and Ar_Individual_Or_Institution = #{type}
    and ar_business_code = '001'
    and rownum = 1
  </select>
  <update id="updateDowloadTime">
    update D_FXQ_UNRESIDENT_DATA set FUD_DOWNLOAD_TIME = #{dowTime} where
    FUD_CUSTOMER_TYPE = #{type,jdbcType=VARCHAR}
    <if test="date != null">
      and FUD_UPLOAD_TIME = #{date,jdbcType=VARCHAR}
    </if>
    <if test="channelCode != null">
      and FUD_CHANNEL_CODE = #{channelCode,jdbcType=VARCHAR}
    </if>
  </update>
</mapper>