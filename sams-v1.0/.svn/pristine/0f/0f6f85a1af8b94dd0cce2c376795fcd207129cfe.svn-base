  <select id="selectAccountCfmData" parameterType="Map" resultType="Map">
  	select 
    ${SQLNAME}
    ,nvl(t1.dict_type,t.arc_business_code) businesscode
    from D_ACCOUNT_REQ_CFM T left join 
(SElect dict_type,dict_code  from sys_dict where dict_pid = ( select dict_id from　sys_dict where dict_type = 'bussinessCode' )) t1 on 
t1.dict_code = t.arc_business_code 
