   <select id="selectExchangeReqCfmData" parameterType="hashMap" resultType="hashMap">
   SELECT <include refid="Base_Column_List_File" />,nvl(t1.dict_type,erc_business_code) businesscode
     FROM D_EXCHANGE_REQ_CFM T left  join 
(select dict_type,dict_code  from sys_dict where dict_pid = ( select dict_id from　sys_dict where dict_type = 'bussinessCode' )) t1 on 
t1.dict_code = t.erc_business_code 
    <where>
    <if test="CHANNELCODE != null" >
       and t.erc_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
    </if>
    <if test="TRANSDATE != null" >
       AND t.erc_transaction_cfm_date = #{TRANSDATE,jdbcType = VARCHAR}
    </if>
    </where>
     order by t.erc_fund_code, t.erc_transaction_account_id 
  </select>
  
  
   <update id="updateGenerateStaAndTime" parameterType="hashMap">
  	update D_EXCHANGE_REQ_CFM set erc_GENERATE_STATUS = '01' ,erc_GENERATE_FILE_TIME = sysdate where erc_channel_code = #{CHANNELCODE,jdbcType = VARCHAR} AND erc_TRANSACTION_cfm_DATE = #{TRANSDATE,jdbcType = VARCHAR}
  </update>
  
  <update id="updateSendStaAndTime" parameterType="hashMap">
  	update D_EXCHANGE_REQ_CFM set erc_SEND_STATUS = '01' , erc_SEND_FILE_TIME = sysdate where erc_channel_code = #{CHANNELCODE,jdbcType = VARCHAR} AND erc_TRANSACTION_cfm_DATE = #{TRANSDATE,jdbcType = VARCHAR} and erc_GENERATE_STATUS = '01'
  </update>
  
  <select id="selectTransStatistics" parameterType="map" resultType="map">
  	select * from (select distinct t.pi_channel_code channelcode,t1.ci_channel_name channelname ,t.pi_channel_product_code fundcode,t.pi_product_type producttype  from p_product_info t ,p_channel_info t1  where t.pi_channel_code = t1.ci_channel_code and t.pi_valid_flag in('01','02') and t.PI_CHECK_STATE = '01' 
  	<if test="CHANNELCODE != null and CHANNELCODE != '' ">
  		and pi_channel_code = #{CHANNELCODE,jdbcType = VARCHAR} 
  	</if>
  	<if test="FUNDCODES != null">
  		and t.pi_channel_product_code in 
  		<foreach collection="FUNDCODES" item="fundCode" index="index" separator="," open="(" close=")">
  		#{fundCode,jdbcType = VARCHAR}
  		</foreach>
  	</if>
  	) t left join (
  	select er_channel_code,er_fund_code,rg_gm rg, sg_gm sg, sh_sh sh from (select t.er_channel_code,t.er_fund_code,t.er_business_code,t.er_application_amount,t.er_application_vol from d_exchange_req t where t.er_return_code = '0000'
  	<if test="STARTDATE != null">
  		and t.er_trans_date &gt;= replace(#{STARTDATE,jdbcType = VARCHAR},'-','')
  	</if>
  	<if test="ENDDATE != null">
  		and t.er_trans_date &lt;= replace(#{ENDDATE,jdbcType = VARCHAR},'-','')
  	</if>
  	) pivot (sum(er_application_vol) sh, sum(er_application_amount) gm for er_business_code in('020' rg,'022' sg,'024' sh)) ) t1 on t1.er_channel_code = t.channelcode and t.fundcode = t1.er_fund_code  
  	left join (select erc_channel_code,erc_fund_code,rg_gm confirmrg,sg_gm confirmsg,sh_sh confirmsh,cL_gm confirmcl,dq_gm confirmdq,qs_gm confirmqs from (SELECT t.erc_channel_code ,t.erc_fund_code,t.erc_business_code,t.erc_confirmed_vol,t.erc_confirmed_amount FROM D_EXCHANGE_REQ_cfm T WHERE t.erc_return_code = '0000'
  	<if test="CONFIRMSTARTDATE != null">
  		and t.erc_trans_date &gt;= replace(#{CONFIRMSTARTDATE,jdbcType = VARCHAR},'-','')
  	</if>
  	<if test="CONFIRMENDDATE != null">
  		and t.erc_trans_date &lt;= replace(#{CONFIRMENDDATE,jdbcType = VARCHAR},'-','')
  	</if>
  	) pivot (sum(erc_confirmed_vol) sh, sum(erc_confirmed_amount) gm for erc_business_code in('020' rg,'022' sg,'024' sh,'130' cl,'142' qs, '150' dq)))	t2 
	on t.channelcode = t2.erc_channel_code and t.fundcode = t2.erc_fund_code
  	order by channelcode
  
  </select>
  
  <select id="queryExchenageSamsSerialnoCount" parameterType="map" resultType="integer">
  	  select count(*)
     from d_exchange_req_cfm t
     where t.erc_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
     and t.erc_trans_date = #{TRANSDATE,jdbcType = VARCHAR}
     group by t.erc_channel_code, t.erc_original_serial_no
     having count(*) > 1
  </select>
  
  <select id="queryFundVolDetailSamsSerialnoCount" parameterType="map" resultType="integer">
  	select count(*)
  	from d_fund_vol_detail_cfm t
 	where t.fvd_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
   	and t.fvd_trans_date = #{TRANSDATE,jdbcType = VARCHAR}
 	group by t.fvd_channel_code, t.fvd_ta_serial_no
	having count(*) > 1
  </select>
  
  
  <select id="getMultipleDetail" parameterType="map" resultType="map">
  	   select nvl(t.er_currency_type, '840') || '(' || (select d.dict_name from sys_dict d where d.dict_type = 'currency' and d.dict_code = nvl(t.er_currency_type, '840')) || ')' currencytype,
       sum(decode(t.er_business_code,
                  '024',
                  t.er_application_vol,
                  t.er_application_amount)) applicationamount
	   from d_exchange_req t
	   where t.er_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
	   and t.er_fund_code = #{FUNDCODE,jdbcType = VARCHAR}
	   and t.er_business_code = #{BUSINESSCODE,jdbcType = VARCHAR}
	   and t.er_return_code = '0000'
	   and t.er_valid_flag = '01'
	   and t.er_trans_date between #{STARTDATE,jdbcType = VARCHAR} and #{ENDDATE,jdbcType = VARCHAR}
	   group by t.er_currency_type
  </select>
  
  <select id="getMultipleCfmDetail" parameterType="map" resultType="map">
  	   select nvl(t.erc_currency_type, '840') || '(' || (select d.dict_name from sys_dict d where d.dict_type = 'currency' and d.dict_code = nvl(t.erc_currency_type, '840')) || ')' currencytype,
       sum(t.erc_confirmed_amount) applicationamount
	   from d_exchange_req_cfm t
	   where t.erc_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
	   and t.erc_fund_code = #{FUNDCODE,jdbcType = VARCHAR}
	   and t.erc_return_code = '0000'
	   and t.erc_business_code = #{BUSINESSCODE,jdbcType = VARCHAR}
	   and t.erc_trans_date between #{STARTDATE,jdbcType = VARCHAR} and #{ENDDATE,jdbcType = VARCHAR}
	   group by t.erc_currency_type
  </select>
</mapper>