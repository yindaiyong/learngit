   <select id="selectCount" parameterType="Map" resultType="java.math.BigDecimal">
   	 select count(*) from d_exchange_req where er_Trans_Date = #{ercTransactionDate,jdbcType=VARCHAR} and ER_TA_TRANS_STATUS in ('454','442','434')  and er_channel_code=#{ercChannelCode,jdbcType=VARCHAR}
  </select>
  
  
 <select id="select94BeforeHandDataCfm" parameterType="hashMap" resultType="hashMap">
   SELECT  ${SQLNAME}
     FROM d_exchange_req T
    WHERE  t.er_business_code = '022'
    <if test="CHANNELCODE != null" >
       AND t.er_channel_code = #{CHANNELCODE,jdbcType = VARCHAR}
    </if>
    <if test="TRANSDATE != null" >
    <!--查询日期为操作日期-->
       AND t.er_trans_date = #{TRANSDATE,jdbcType = VARCHAR}
    </if>
    <if test="FUNDTYPES != null ">
     and exists (
		     select 1 from p_product_info p 
		     where p.pi_channel_code = er_channel_code 
		     and p.pi_channel_product_code = er_fund_code  
		     and p.pi_check_state = '01' 
		     and p.pi_valid_flag not in ('00','99') 
		     and pi_product_type  in 
    <foreach collection="FUNDTYPES" item="id" open="(" separator="," close=")">
  		#{id}
    </foreach>
    )
    </if>
    order by t.er_fund_code,t.er_transaction_account_id 
  </select>
  
